#!/usr/bin/env bash
# Import training_log.ndjson into TensorBoard format (requires: pip install -e ".[rl]")
# Usage: bun run import-tb -- /tmp/rl-run/training_log.ndjson
cd "$(dirname "$0")/.."
if [ -x .venv/bin/python3 ]; then
  exec .venv/bin/python3 -m mage_knight_sdk.tools.import_tensorboard "$@"
else
  exec python3 -m mage_knight_sdk.tools.import_tensorboard "$@"
fi
